	"dependencies": {
		"webpack": "^5.65.0",
		"webpack-stream": "^7.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*": [
			"prettier --write --ignore-unknown"
		],
		"*.scss": [
			"stylelint --fix --allow-empty-input"
