	"license": "GPL-3.0",
	"version": "0.0.1",
	"main": "./gulpfile.js",
	"preview": false,
	"galleryBanner": {
		"color": "#20232a",
		"theme": "dark"
	},
	"categories": ["Other", "Visualization"],
	"repository": {
		"type": "git",
		"url": "https://github.com/kah3vich/Gulp-RS"
	},
	"bugs": {
		"url": "https://github.com/kah3vich/Gulp-RS/issues"
	},
	"homepage": "https://kah3vich.github.io/Gulp-RS/public/index.html",
